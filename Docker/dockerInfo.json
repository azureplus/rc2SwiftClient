{
	"containers": [
		{
			"Image": "rc2server/combined",
			"Hostname": "combined",
			"ExposedPorts": { "8088/tcp": {} },
			"Labels": { "rc2.live": "" },
			"HostConfig": {
			   "Binds": ["rc2_dbdata:/rc2"],
				"Tmpfs": { "/tmp": "rw,noexec,nosuid,size=64M" },
				"PortBindings": { "8088/tcp": [{ "HostPort": "3145" }] },
				"LogConfig": { "Type": "json-file", "Config": { "max-file": "2", "max-size": "500k" } }
			}
		}
	]
}
